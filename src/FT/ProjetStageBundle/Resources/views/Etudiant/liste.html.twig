{% extends "FTProjetStageBundle::index.html.twig" %}

{% block body %}
    <div class="container">
        <h2>Liste des étudiants</h2>
        <table class="table">
            <thead>
            <tr>
                <th>Nom</th>
                <th>Prénom</th>
                <th>Email</th>
            </tr>
            </thead>
            <tbody>
            {% for etudiant in listeEtudiants %}
                <tr>
                    <td>{{ etudiant.id }}</td>
                    <td>Doe</td>
                    <td>john@example.com</td>
                </tr>
            {% endfor %}
            </tbody>
        </table>
    </div>
{% endblock %}